{
  "name": "stark-sequencer",
  "version": "1.0.0",
  "description": "A scheduler and monitor for TypeScript in soft real-time.",
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "esbuild"
  ],
  "browser": "./browser.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build package",
    "build-all": "yarn clean && yarn install",
    "build:dev": "esbuild src/index.ts --bundle --outfile=dist/index.js",
    "build:watch": "esbuild src/index.ts --bundle --watch --outfile=dist/index.js",
    "postinstall": "node build.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "browser-env-vars": "^1.1.1",
    "esbuild": "^0.11.23",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "browser-assert": "^1.2.1",
    "events": "^3.3.0",
    "flat-promise": "^1.0.2",
    "lodash": "^4.17.21",
    "objectmodel": "^4.3.0",
    "timesync": "^1.0.8",
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  }
}
